  "license": "MIT",
  "devDependencies": {
    "@semantic-release/npm": "^7.0.6",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "autoprefixer": "^8.6.5",
    "postcss": "^8.1.1",
    "semantic-release": "^17.2.1"
  }
